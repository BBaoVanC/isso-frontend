import React from "react";

type CommentProps = {
  content: string;
}
type CommentState = {
  count: number;
};
export class Comment extends React.Component<CommentProps, CommentState> {
  state: CommentState = {
    count: 0,
  }
  render() {
    return (
      <div>
        {this.props.content} -- count: {this.state.count}
      </div>
    );
  }
}
